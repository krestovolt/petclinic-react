{
	"css.validate": false,
	"docthis.includeMemberOfOnClassMembers": false,
	"docthis.includeMemberOfOnInterfaceMembers": false,
	"docthis.includeTypes": false,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80],
  "editor.tabSize": 2,
  "eslint.enable": false,
  "files.insertFinalNewline": true,
  "less.validate": false,
  "licenser.license": "AL2",
  "licenser.projectName": "Pet Clinic React",
  "licenser.useSingleLineStyle": false,
  "prettier.printWidth": 80,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.useTabs": false,
  "scss.validate": false,
  "tslint.enable": true,
  "tslint.exclude": ["**/dist/**", "**/node_modules/**"],
  "tslint.ignoreDefinitionFiles": true,
  "tslint.jsEnable": true,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "xo.enable": false
}
